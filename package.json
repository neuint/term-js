{
  "name": "termjs",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.js",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js",
    "test": "jest",
    "test-coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nc-kage/termjs.git"
  },
  "author": "Pavlo Gusar",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.html?$": "html-loader-jest",
      "^.+\\.(css|scss)?$": "<rootDir>/tests/__mocks__/styleMock.js"
    },
    "moduleNameMapper": {
      "^@Term/(.*)$": "<rootDir>/src/Term/$1",
      "^lodash-es$": "lodash"
    }
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.1",
    "@types/webpack-env": "^1.15.1",
    "autoprefixer": "^9.7.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-loader-jest": "^0.2.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.4",
    "ts-jest": "^25.1.0",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2"
  }
}
